{
    "name": "ajgarlag/psr15-dispatcher",
    "description": "Component to dispatch PSR-15 middlewares",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio J. GarcÃ­a Lagar",
            "email": "aj@garcialagar.es"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.0",
        "psr/http-message": "^1",
        "http-interop/http-middleware": "0.4.*"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2",
        "friendsofphp/php-cs-fixer": "^1.13",
        "http-interop/http-factory-diactoros": "0.1.*"
    },
    "autoload": {
        "psr-4": {
            "Ajgarlag\\Psr15\\Dispatcher\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Ajgarlag\\Psr15\\Dispatcher\\": "./spec"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpspec run",
        "fix-cs": "vendor/bin/php-cs-fixer fix; vendor/bin/php-cs-fixer fix --config-file=spec/.php_cs"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
